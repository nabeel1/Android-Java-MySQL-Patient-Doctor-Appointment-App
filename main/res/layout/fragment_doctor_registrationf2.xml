<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context=".Registrations.doctor_registrationf2">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="20dp"
        android:orientation="vertical">
        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <AutoCompleteTextView
                android:id="@+id/workexperience"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:drawableLeft="@drawable/experience"
                android:drawablePadding="10dp"
                android:hint="Work Experience"
                android:inputType="textMultiLine"
                android:textColorHint="#000000" />

        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <AutoCompleteTextView
                android:id="@+id/aboutme"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:drawableLeft="@drawable/about"
                android:drawablePadding="10dp"
                android:hint="Tell us About yourself"
                android:inputType="textMultiLine"
                android:textColorHint="#000000" />

        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <AutoCompleteTextView
                android:id="@+id/Specialization"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:drawableLeft="@drawable/specialization"
                android:drawablePadding="10dp"
                android:hint="Specialization"
                android:inputType="text"
                android:textColorHint="#000000" />

        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <AutoCompleteTextView
                android:id="@+id/qualification"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:drawableLeft="@drawable/qualification"
                android:drawablePadding="10dp"
                android:hint="Qualifications"
                android:inputType="textMultiLine"
                android:textColorHint="#000000"  />

        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <AutoCompleteTextView
                android:id="@+id/experienceYear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:drawableLeft="@drawable/time"
                android:drawablePadding="10dp"
                android:hint="Experience Year"
                android:inputType="number"
                android:textColorHint="#000000" />

        </android.support.design.widget.TextInputLayout>

    </LinearLayout>

</FrameLayout>
